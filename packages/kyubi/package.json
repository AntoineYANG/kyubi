{
  "name": "kyubi-js",
  "version": "0.1.0-alpha.6",
  "license": "MIT",
  "description": "A website engine for the modern docs, blogs, wikis, and more. Based on MDX and Next.js.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json",
    "./loader": "./dist/loader.js",
    "./layout": "./dist/layout/index.js",
    "./elements/*": {
      "import": "./dist/elements/*.mjs",
      "require": "./dist/elements/*.js"
    },
    "./index.css": "./dist/index.css"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn build:core && yarn build:layout",
    "build:core": "tsup",
    "build:layout": "postcss src/layout/index.css -o dist/index.css",
    "dev:core": "tsup --watch",
    "dev:layout": "postcss src/layout/index.css -o dist/index.css --watch",
    "dev": "yarn dev:core & yarn dev:layout"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/next": "^9.0.0",
    "@types/node": ">=18",
    "@types/react": ">=17",
    "@types/react-dom": "^18.2.19",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.17",
    "next": "^14.1.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "chalk": "^5.3.0",
    "gray-matter": "^4.0.3",
    "react-aria": "^3.32.0",
    "react-stately": "^3.30.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
